{
  "private": true,
  "name": "input-assist-lean",
  "displayName": "Input Assist (Lean)",
  "description": "Tree-based input assistance for unicode and more in Visual Studio Code",
  "version": "0.0.6",
  "publisher": "jroesch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jroesch/vscode-input-assist/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jroesch/vscode-input-assist.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [],
  "activationEvents": [
    "*"
  ],
  "files": [
    "assets"
  ],
  "main": "./out/src/main",
  "contributes": {
    "configuration": {
      "title": "Input Assist configuration",
      "properties": {
        "input-assist.languages": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              },
              "uniqueItems": true
            }
          ],
          "default": null,
          "description": "List of language ids for which to enable input assistance"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "tslint": "^3.15.1",
    "typescript": "^2.0.9",
    "vscode": "^1.0.3"
  },
  "dependencies": {},
  "extensionDependencies": []
}
